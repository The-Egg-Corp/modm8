{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./node_modules",
    "reinstall": "bun run clean && bun i",
    "copy-themes": "copy-folder ./node_modules/primevue/resources/themes ./src/assets/themes --summary",
    "dev": "bun run copy-themes && bunx vite",
    "build": "bun run copy-themes && bunx vite build",
    "typecheck": "bunx vue-tsc --noEmit",
    "ci": "bun i && bun run typecheck"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "compare-versions": "^6.1.0",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "vue": "^3.4.26",
    "vue-i18n": "9",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "@types/node": "^22.2.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "copy-folder-util": "^1.1.3",
    "rimraf": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue-tsc": "latest"
  }
}
